<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:context="com.faceunity.pta_art.SelectStyleActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/x140"
        android:text="请选择模型生成风格"
        android:textColor="@color/colorBlack"
        android:textSize="@dimen/text_size_32" />

    <ImageView
        android:id="@+id/select_style_new"
        android:layout_width="@dimen/x414"
        android:layout_height="@dimen/x416"
        android:layout_marginTop="@dimen/x120"
        android:onClick="onClick"
        android:padding="@dimen/x50"
        android:scaleType="centerCrop"
        android:src="@drawable/select_style_new" />

    <ImageView
        android:id="@+id/select_style_art"
        android:layout_width="@dimen/x414"
        android:layout_height="@dimen/x418"
        android:layout_marginTop="@dimen/x60"
        android:onClick="onClick"
        android:padding="@dimen/x50"
        android:scaleType="centerCrop"
        android:src="@drawable/select_style_art"
        android:text="new" />
</LinearLayout>
